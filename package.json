{
  "name": "overflow-api-v1",
  "version": "1.0.0",
  "description": "Stack overflow lite application",
  "engines": {
    "node": "8.0"
  },
  "main": "app.js",
  "scripts": {
    "build": "babel app.js -d dist && babel swagger.json -d dist && babel server -d dist/server ",
    "test": " nyc mocha --exit --timeout 25000 ./server/test/**/*.js --compilers js:babel-core/register",
    "babel-node": "babel-node app --presets=env",
    "start": "node dist/app.js",
    "start:dev": "nodemon --exec babel-node app.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc npm test&& nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/julietezekwe/overflow-api-v1.git"
  },
  "author": "Juliet",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/julietezekwe/overflow-api-v1/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^9.0.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "chai-http": "^4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "nyc": "^13.0.1",
    "request": "^2.88.0"
  },
  "homepage": "https://github.com/julietezekwe/overflow-api-v1#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-validator": "^5.3.0",
    "jsonwebtoken": "^8.3.0",
    "swagger-ui-express": "^4.0.1"
  }
}
